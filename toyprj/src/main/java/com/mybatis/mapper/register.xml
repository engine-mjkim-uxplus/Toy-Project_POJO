<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mybatis.mapper.registerMapper">
	<insert id="registerInsert" parameterType="map">
		INSERT INTO MEMBER ("member_id", "memeber_pw", "member_name", "member_phone", "member_zipcode"
						  , "member_address", "member_address2", "member_gender")
     				VALUES (#{member_id}, #{member_pw}, #{member_name}, #{member_phone}, #{member_zipcode}
     					  , #{member_address}, #{member_address2}, #{member_gender})
	</insert>
</mapper>